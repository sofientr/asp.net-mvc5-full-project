@model IEnumerable<Inspinia_MVC5.Models.ParametrageSemaines>
@{
    ViewBag.Title = "View";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>View</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("List", "Index")
            </li>
            <li class="active">
                <strong>View</strong>
            </li>
        </ol>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary" })
        </div>
    </div>
</div>



<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>View ParametrageSemaine</h5>
                </div>
                <div class="ibox-content">
                    <div class="ibox-content">

                        <table class="table table-striped table-bordered table-hover dataTables-example">
                            <thead>
                                <tr>
                                    <th>
                                        Jour
                                    </th>
                                    <th>
                                        Jour de travail
                                    </th>
                                    <th>
                                        Double Séance
                                    </th>
                                    <th>
                                        Seance 1 début
                                    </th>
                                    <th>
                                        Séance 1 fin
                                    </th>
                                    <th>
                                        Séance 2 début
                                    </th>
                                    <th>
                                        Séance 2 fin
                                    </th>
                                    <th>

                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        @using (Html.BeginForm())
                                        {
                                            @Html.AntiForgeryToken()

                                           // @Html.ValidationSummary(true)
                                            @Html.HiddenFor(it => item.Id )
                                            @Html.HiddenFor(it => item.JourId)
                                            @Html.HiddenFor(it=>item.projetTechniqueID)
                                            @*<td>
                                                @Html.TextBoxFor(it => item.jourLibelle, new { disabled = "disabled", @readonly = "readonly" })
                                                @Html.ValidationMessageFor(it => item.JourLibelle)
                                            </td>*@
                                            <td>
                                                @Html.EditorFor(it => item.jourTravail)
                                                @Html.ValidationMessageFor(it => item.jourTravail)
                                            </td>
                                            <td>
                                                @Html.EditorFor(it => item.doubleSeance)
                                                @Html.ValidationMessageFor(it => item.doubleSeance)
                                            </td>
                                            <td>
                                                @Html.EditorFor(it => item.seance1Debut)
                                                @Html.ValidationMessageFor(it => item.seance1Debut)
                                            </td>
                                            <td>
                                                @Html.EditorFor(it => item.seance1Fin)
                                                @Html.ValidationMessageFor(it => item.seance1Fin)
                                            </td>
                                            <td>
                                                @Html.EditorFor(it => item.seance2Debut)
                                                @Html.ValidationMessageFor(it => item.seance1Debut)
                                            </td>
                                            <td>
                                                @Html.EditorFor(it => item.seance2Fin)
                                                @Html.ValidationMessageFor(it => item.seance2Fin)
                                            </td>
                                            <td>
                                                <input type="submit" value="Save" class="btn btn-primary" />
                                            </td>
                                        }
                                    </tr>

                                }
                            </tbody>
                        </table>


                        @*<div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Save" class="btn btn-primary" />
                                @Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-white" })
                            </div>
                        </div>*@
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/jquery-2.1.1.min.js">

</script>
